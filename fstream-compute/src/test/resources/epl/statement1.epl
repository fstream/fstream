SELECT
   current_timestamp.toDate(),
   a.symbol,
   a.dateTime,
   b.symbol,
   b.dateTime,
   c.symbol,
   c.dateTime
FROM 
   PATTERN [
         Rate(symbol='AUD/USD') OR
         Rate(symbol='AUD/NZD') OR 
         Rate(symbol='NZD/USD')
      ->
         a = Rate(symbol='AUD/USD') AND 
         b = Rate(symbol='AUD/NZD') AND 
         c = Rate(symbol='NZD/USD')
     
      WHERE
         timer:within(6 sec)
   ] 
