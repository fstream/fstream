[program:{{ fstream_module }}]
command=java -XX:MaxPermSize=128m -cp /usr/local/etc/{{ fstream_module }}-{{ fstream_version }}/conf:/usr/local/etc/{{ fstream_module }}-{{ fstream_version }}/lib/{{ fstream_module }}-{{ fstream_version }}.jar {{ main_class }} --server.port={{ http_port }} --zk.port=2181 --kafka.port=9092 --storm.local=false --rates.password={{ rates_password }} start-foreground
priority={{ priority }}
user={{fstream_user}}
autostart=true
autorestart=true
startsecs=10
startretries=999
log_stdout=true
log_stderr=true
logfile=/var/log/{{ fstream_module }}/{{ fstream_module }}.out
logfile_maxbytes=20MB
logfile_backups=10
